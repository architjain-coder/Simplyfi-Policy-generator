{
  "manifest_version": 2 ,
  "name": "Policy Extesnion",
  "version": "0.1.1",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "description": "Find best candidates for jobs at your company without pro-active searching",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Policy classifer",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}

